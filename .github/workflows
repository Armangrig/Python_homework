name: mydocker
on: 
  push:
jobs:
  docker1:
   
   strategy:
      matrix:
        version: [10, 12, 14]
        os: [ubuntu-latest]
        
   runs-on: ${{ matrix.os }}
   steps:
    - uses: actions/checkout@v2

    - name: Login to Docker Hub
      run: |
       docker login -u ${{ secrets.DOCKERHUB_USERNAME }} -p ${{ secrets.DOCKERHUB_TOKEN }}
                  
    - name: Build and push
      uses: docker/build-push-action@v3
      
      with:
        push: true
        tags: armangrig88/golang:latest
